# Anthropic Claude API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Application Configuration
NODE_ENV=development
PORT=3000

# Model Configuration (demo)
# Primary model used by the Collaborator agent (streaming)
COLLABORATOR_MODEL=claude-3-5-haiku-latest
# Optional: Memory Keeper model; if unset, server will use COLLABORATOR_MODEL
# MEMORY_MODEL=claude-3-5-haiku-latest

# Optional: Rate limiting and session management
SESSION_SECRET=your_session_secret_here
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Optional: Logging and monitoring
LOG_LEVEL=info
ENABLE_ANALYTICS=false
